<div class="ea-property-detail">
    {% if field.value is not empty %}
        {% for image in field.value %}
            <img src="{{ vich_uploader_asset(image, 'imageFile') }}" class="img-thumbnail" style="max-width: 100px; max-height: 100px; margin-right: 5px;" alt="Property Image">
        {% endfor %}
    {% else %}
        No images
    {% endif %}
</div>
